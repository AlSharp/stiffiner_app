<p id="notice"><%= notice %></p>

<h1>Stiffeners</h1>

<table>
  <thead>
    <tr>
      <th>Pos</th>
      <th>Height</th>
      <th>Width</th>
      <th>Chamfer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stiffeners.each do |stiffener| %>
      <tr>
        <td><%= stiffener.Pos %></td>
        <td><%= stiffener.Height %></td>
        <td><%= stiffener.Width %></td>
        <td><%= stiffener.Chamfer %></td>
        <td><%= link_to 'Show', stiffener %></td>
        <td><%= link_to 'Edit', edit_stiffener_path(stiffener) %></td>
        <td><%= link_to 'Destroy', stiffener, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stiffener', new_stiffener_path %>
